<odoo>

    <record model="ir.ui.view" id="product_product_form_view">
        <field name="name">product.product.form (in fleet_vehicle_model_part)</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view" />
        <field name="arch" type="xml">
            <page name="fleet" position="inside">
                <group string="Vehicle Models" name="vehicle_models">
                        <field name="vehicle_model_ids" nolabel="1">
                            <tree>
                                <field name="name" />
                            </tree>
                        </field>
                    </group>
                </page>
        </field>
    </record>

</odoo>
